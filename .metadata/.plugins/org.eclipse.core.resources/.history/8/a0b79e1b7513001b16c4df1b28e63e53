import java.util.Scanner;

public class Quiz0606
{

	public static void main(String[] args)
	{
		System.out.print("몇 개의 정수를 입력하시겠습니까? : ");
		Scanner sc = new Scanner(System.in);
		
		int num1 = sc.nextInt();
		System.out.printf("%d개의 정수를 입력하세요. : \n",num1);
		int num2 = sc.nextInt();
		num2 = num1;
	}

}
