
public class Quiz0604
{

	public static void main(String[] args)
	{
		int num = 1;
		int result = 0;
		
		int Max = 10;
		do
		{
			if(num < Max)
			{
				System.out.print(num + " + ");
				
			}
			else
			{
				System.out.print(" = ");
			}
			num++;
		}while(num <= Max);
		result = result + num;
		System.out.println(result);
	}
}

//do 
//{	
//} while(조건);
