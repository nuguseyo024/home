package Game;

import java.util.Random;

public class B_RandomNumber
{
	
	public static void main(String[] args)
	{
		Random randomV1 = new Random();
		
		int n100 = (randomV1.nextInt(8))+1;
		int n10 = (randomV1.nextInt(8))+1;
		int n1 = (randomV1.nextInt(8))+1;
		while(n100 != n10 && n100 != n1) 
		{
			if(n10 != n1) {
				System.out.printf("%d %d %d", n100, n10, n1);break;}	
		}
	}

}
