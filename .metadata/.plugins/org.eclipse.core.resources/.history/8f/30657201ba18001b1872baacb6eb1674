import java.util.Scanner;

public class test {

	public static void main(String[] args) {
		/* 필요한 변수 선언 및 초기화 */
		int[] num = new int[10];

		Scanner sc = new Scanner(System.in);

		System.out.printf("10개의 숫자를 입력하세요.\n");

		for (int i = 0; i < num.length; i++) {
			num[i] = sc.nextInt();
		}

		System.out.print("홀수 : ");
		for (int i = 0; i < num.length; i++) {
			if ((num[i] % 2) == 1) { // 입력한 숫자가 홀수인 경우
				System.out.print(" " + num[i] + " ");
			}
		}

		System.out.print("\n짝수 : ");
		for (int i = 0; i < num.length; i++) {
			if ((num[i] % 2) == 0) { // 입력한 숫자가 짝수인 경우
				System.out.print(" " + num[i] + " ");
			}
		}

	}

}


