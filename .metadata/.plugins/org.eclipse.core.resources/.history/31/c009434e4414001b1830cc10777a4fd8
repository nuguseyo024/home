package Quiz07;

public class Quiz0701
{

	public static void main(String[] args)
	{
		int result;
		result = JJ(5, 10, 15);
		
		System.out.println("nResult : " + result);
	}
	public static int JJ(int n1,int n2,int n3)
	{
		int nResult = ((n1+n2+n3)/3);
		return nResult;
	}

}
//문제1
//세 개의 정수를 인자로 전달 받아서 평균을 반환하는 함수를 정의하라
