package Quiz07;

public class Quiz0702_ssam
{

	public static void main(String[] args)
	{
		int max = getMax(5,9,7);
		getMax(1,10,3);
		
	}
	public static void getMax(int n1,int n2,int n3)
	{
		int max = 0;
		if(n1 > n2)
		{
			max = n1;
		}
		else
		{
			max = n2;
		}
		if(max < n3)
		{
			max = n3;
		}
		
		return max;
	}

}
//문제2
//세 개의 정수를 인자로 전달 받아서 그중 가장 큰 수를 반환하는 함수를 정의하라
