
public class E3_PartiallyFilledArray
{

	public static void main(String[] args)
	{
		int[][] arr = 
		{
			{11},
			{22, 33},
			{44, 55, 66}
		};
		
		// 배열의 구조대로 내용 출력
		for(int i = 0; i < arr.length; i++) //length 를 뽑으면 앞 배열 갯수
		{
			for(int j = 0; j < arr[i].length; j++) //[].length를 뽑으면 각각의 갯수
				System.out.print(arr[j][i] + "\t");
			System.out.println();
		}
	}

}

