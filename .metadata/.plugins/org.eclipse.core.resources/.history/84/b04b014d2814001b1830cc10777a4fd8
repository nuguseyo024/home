import java.util.Scanner;
public class Aaaa
{

	public static void main(String[] args)
	{
		Scanner sc = new Scanner(System.in);
		System.out.println("사용자로부터 5개의 정수를 입력받아 합을 출력");
		
		System.out.println("정수 5개를 입력해주세요: ");
		int num1 = sc.nextInt();
		int num2 = sc.nextInt();
		int num3 = sc.nextInt();
		int num4 = sc.nextInt();
		int num5 = sc.nextInt();
		
		int result = 0;
		
		while (true)
		{
			if (num1 < 1)
			{
				System.out.println("1번값 오류. 1이상의 정수를 입력해주세요");
				num1 = sc.nextInt();
			}
			else if (num2 < 1)
			{
				System.out.println("2번값 오류. 1이상의 정수를 입력해주세요");
				num2 = sc.nextInt();
			}
			else if (num3 < 1)
			{
				System.out.println("3번값 오류. 1이상의 정수를 입력해주세요");
				num3 = sc.nextInt();
			}
			else if (num4 < 1)
			{
				System.out.println("4번값 오류. 1이상의 정수를 입력해주세요");
				num4 = sc.nextInt();
			}
			else if (num5 < 1)
			{
				System.out.println("5번값 오류. 1이상의 정수를 입력해주세요");
				num5 = sc.nextInt();
			}
			else
			{
				result = (num1 + num2 + num3 + num4 + num5);
				break;
			}
			
		}
		
		sc.close();
		System.out.println("5개 정수의 합: " + result);
		
	}

}
