package Quiz13;

public class Quiz13_7
{

	public static void main(String[] args)
	{
		int arr[][] = {{1,2,3,4},{1,2,3,4}};
		for(int i = 0; i < arr.length; i++){
			System.out.print(i+"\t");
			for(int j = 0; j < arr[i].length; j++)
			{
				
				System.out.print(j+"\t");
				System.out.println();
				System.out.print(arr[i][j]+"\t");
			}
		}
			
	}

}
