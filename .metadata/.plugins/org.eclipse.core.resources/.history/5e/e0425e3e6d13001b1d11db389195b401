
public class Quiz0604
{

	public static void main(String[] args)
	{
		int num = 1;
		int result;
		result = num + num;
		do
		{
			if(num < 10)
			{
				System.out.print(num + " + ");
				num++;
			}
			else
			{ 
				System.out.println(" = " + result);
			}
		}
			
		
		while(num <= 10);
		
		
		
		
	}
}
