class Book
{
	int price;
	int num = 0;
	String title;
	
	Book()
	{
		title = "자바 프로그래밍";
		price = 5000;
	}
	Book(String t, int p)
	{
		title = t;
		price = p;
	}
	void print()
	{
		System.out.println(" 제    목 : " + title);
		System.out.println(" 가    격 : " + price);
		System.out.println(" 주문수량 : " + num);
		System.out.println(" 합계금액 : " + price*num);
	}
}
class MyBook
{
	public static void main(String[] args)
	{ 
		Book bk2 = new Book();
		bk2.print();
		
		Book bk1 = new Book("자바 디자인패턴", 10000);
		bk1.num = 10;
		bk1.print();
	}
}

