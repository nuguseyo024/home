
public class E1_TwoDimenArray
{

	public static void main(String[] args)
	{
		int[][]arr = new int[10][10];
		
		int num = 1;
		
		for(int i = 0; i < 3; i++)
		{
			for(int j = 0; j < 4; j++)
			{
				arr[i][j] = num;
				num++;
			}
		}
		
		for(int i = 0; i < 3; i++)
		{
			for(int j = 0; j < 4; j++)
				System.out.print(arr[j][i] + "\t");
			System.out.println();
		}
	}

}
// }:|
