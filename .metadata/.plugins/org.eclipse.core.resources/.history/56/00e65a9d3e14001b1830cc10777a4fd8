package Quiz07;

public class Quiz0702
{

	public static void main(String[] args)
	{
		Small(1,10,3);
	}
	public static void Small(int n1,int n2,int n3)
	{
		if(n1<n2 && n1<n2)
			System.out.println(n1);
		else if(n2<n1 && n2<n3)
			System.out.println(n2);
		else if(n3<n1 && n3<n2)
			System.out.println(n3);
	}

}
