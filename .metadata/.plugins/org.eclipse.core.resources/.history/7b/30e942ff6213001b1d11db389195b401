import java.util.Scanner;

public class Quiz0603
{

	public static void main(String[] args)
	{
		Scanner sc = new Scanner(System.in);
		
		System.out.print("정수 하나를 입력하시오 : ");
		int num1 = sc.nextInt();
		int result;
		
		while(num1 > 1)
		{
			int n = num1--;
			result = (num1 * n--);
			if(n-- == 0)
				break;
			System.out.println(result);
		}
		
		
		
	}

}
