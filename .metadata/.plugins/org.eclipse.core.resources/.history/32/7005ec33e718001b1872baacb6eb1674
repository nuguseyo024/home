package Quiz13;

public class Quiz13_7
{

	public static void main(String[] args)
	{
		int arr[][] = new int[4][4];
		
		int num = 1;
		
		for(int i = 0; i < 4; i++)
		{
			for(int j = 0; j < 4; j++)
			{
				arr[i][j] = num;
				num++;
			}
		}
		for(int i = 0; i < 4; i++)
		{
			for(int j = 0; j < 4; j++)
			{
				System.out.printf("%d  %d ",i,j);
			}
		}
		


	}

}
