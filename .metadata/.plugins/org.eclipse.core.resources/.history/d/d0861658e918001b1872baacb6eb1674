package Quiz13;

public class Quiz13_7
{

	public static void main(String[] args)
	{
		int arr[][] = {{1,2,3,4},{1,2,3,4}};
		for(int i = 0; i < arr.length; i++){
			for(int j = 0; j < arr[i].length; j++)
			{
				System.out.println(i);
				System.out.println(j);
				System.out.println(arr[i][j]);
			}
		}
			
	}

}
