class Book{
	
	String title;
	String author;
	int money;
}
public class MyBook
{

	public static void main(String[] args)
	{
		Book bk = new Book();
		bk.title = "자바 프로그래밍";
		bk.author = "홍길동";
		bk.money = 15000;
		
		System.out.println( bk.title + " : " + 
							bk.author + " : " +
							bk.money);
		
	}

}
